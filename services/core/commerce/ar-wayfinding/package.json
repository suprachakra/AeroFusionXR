  "version": "1.0.0",
  "description": "AR Wayfinding & Navigation service for AeroFusionXR platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
