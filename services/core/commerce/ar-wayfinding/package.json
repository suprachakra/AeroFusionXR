    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0"
  },
  "peerDependencies": {
    "@aerofusionxr/notifications": "^1.0.0",
    "@aerofusionxr/localization": "^1.0.0",
