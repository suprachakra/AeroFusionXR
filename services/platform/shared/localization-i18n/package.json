{
  "name": "@aerofusionxr/localization-i18n",
  "version": "1.0.0",
  "description": "Advanced localization and internationalization service for AeroFusionXR with RTL support and CLDR formatting",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t aerofusionxr/localization-i18n .",
    "docker:run": "docker run -p 8084:8084 aerofusionxr/localization-i18n",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "i18n:extract": "i18next-scanner --config i18next-scanner.config.js",
    "i18n:sync": "node scripts/sync-translations.js",
    "i18n:validate": "node scripts/validate-translations.js"
  },
  "keywords": [
    "localization",
    "internationalization",
    "i18n",
    "l10n",
    "translation",
    "rtl",
    "cldr",
    "formatting",
    "multilingual",
    "airport",
    "commerce"
  ],
  "author": "AeroFusionXR Team",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
