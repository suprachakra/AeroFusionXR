{
  "name": "@aerofusionxr/monitoring-observability",
  "version": "1.0.0",
  "description": "Advanced monitoring and observability service for AeroFusionXR with Prometheus metrics, OpenTelemetry tracing, ELK logging, and synthetic monitoring",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t aerofusionxr/monitoring-observability .",
    "docker:run": "docker run -p 8089:8089 aerofusionxr/monitoring-observability",
    "prometheus:start": "prometheus --config.file=/etc/prometheus/prometheus.yml",
    "grafana:start": "grafana-server --config=/etc/grafana/grafana.ini",
    "jaeger:start": "jaeger-all-in-one --collector.otlp.enabled=true",
    "synthetic:run": "node scripts/run-synthetic-tests.js",
    "alerts:validate": "promtool check rules /etc/prometheus/rules/*.yml",
    "dashboards:import": "node scripts/import-dashboards.js"
  },
  "keywords": [
    "monitoring",
    "observability",
    "prometheus",
    "grafana",
    "jaeger",
    "opentelemetry",
    "elasticsearch",
    "kibana",
    "alerting",
    "synthetic-monitoring",
    "metrics",
    "tracing",
    "logging",
    "airport",
    "commerce"
  ],
