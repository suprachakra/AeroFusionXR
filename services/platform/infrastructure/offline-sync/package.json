{
  "name": "@aerofusionxr/offline-sync",
  "version": "1.0.0",
  "description": "Advanced offline synchronization service for AeroFusionXR with background sync and conflict resolution",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t aerofusionxr/offline-sync .",
    "docker:run": "docker run -p 8085:8085 aerofusionxr/offline-sync",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "sync:test": "node scripts/test-sync.js",
    "cache:cleanup": "node scripts/cleanup-cache.js",
    "queue:process": "node scripts/process-queue.js"
  },
  "keywords": [
    "offline",
    "synchronization",
    "cache",
    "background-sync",
    "conflict-resolution",
    "data-sync",
    "progressive-web-app",
    "service-worker",
    "airport",
    "commerce"
  ],
