    "react-native-slider": "^0.11.0",
    "react-native-progress": "^5.0.1",
    "react-native-animatable": "^1.3.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-shadow-2": "^7.0.8",
    "lottie-react-native": "^6.4.1",
    "react-native-fast-image": "^8.6.3",
    "react-native-image-crop-picker": "^0.39.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-picker-select": "^8.1.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-phone-number-input": "^2.1.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-swiper": "^1.6.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-super-ellipse-mask": "^1.0.2",
    "react-native-element-dropdown": "^2.10.1",
    "@react-native-community/checkbox": "^0.5.16",
    "@react-native-community/slider": "^4.4.2",
    "react-native-collapsible": "^1.6.1",
    "react-native-accordion": "^1.1.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-pager-view": "^6.2.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-calendars": "^1.1301.0",
    "react-native-timeline-listview": "^0.2.3",
    "react-native-step-indicator": "^1.0.3",
    "react-native-dotenv": "^3.4.9",
    "react-native-config": "^1.5.1",
    "react-native-uuid": "^2.0.1",
    "react-native-crypto-js": "^1.0.0",
    "react-native-jwt-decode": "^1.0.0",
    "axios": "^1.5.1",
    "socket.io-client": "^4.7.2",
    "lodash": "^4.17.21",
    "dayjs": "^1.11.10",
    "fuse.js": "^7.0.0",
    "validator": "^13.11.0",
    "yup": "^1.3.3",
    "react-hook-form": "^7.47.0",
    "@hookform/resolvers": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/lodash": "^4.14.200",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "@testing-library/react-native": "^12.3.2",
    "@testing-library/jest-native": "^5.4.3",
    "detox": "^20.13.5",
    "flipper-plugin-react-query-native-devtools": "^4.0.0",
    "flipper-plugin-redux-debugger": "^0.8.9",
    "reactotron-react-native": "^5.0.4",
    "reactotron-redux": "^3.1.9",
    "reactotron-flipper": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-navigation|@react-navigation|react-native-reanimated|react-native-gesture-handler|react-native-screens|react-native-safe-area-context|react-native-vector-icons|@react-native-firebase|@notifee|react-native-maps|@react-native-mapbox-gl)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "keywords": [
    "react-native",
    "airport",
    "navigation",
    "ar",
    "wayfinding",
    "mobile",
    "ios",
    "android"
  ],
  "author": {
    "name": "AeroFusionXR Team",
    "email": "team@aerofusionxr.com",
    "url": "https://aerofusionxr.com"
  },
  "license": "MIT",
  "repository": {
