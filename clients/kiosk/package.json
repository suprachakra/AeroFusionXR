{
  "name": "@aerofusionxr/kiosk-client",
  "version": "2.0.0",
  "description": "AeroFusionXR Kiosk Client - Self-service airport terminal interface",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "serve": "serve -s dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.11.15",
    "@mui/icons-material": "^5.11.16",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@tanstack/react-query": "^4.28.0",
    "framer-motion": "^10.12.4",
    "socket.io-client": "^4.6.1",
    "react-qr-code": "^2.0.11",
    "react-barcode": "^1.4.6",
    "leaflet": "^1.9.3",
    "react-leaflet": "^4.2.1",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "axios": "^1.3.4",
    "react-spring": "^9.7.1",
    "use-gesture": "^9.1.3",
    "react-intersection-observer": "^9.4.3",
    "react-use": "^17.4.0",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/leaflet": "^1.9.3",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^0.14.7",
    "typescript": "^5.0.2",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.7",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "serve": "^14.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "kiosk",
    "airport",
    "self-service",
    "terminal",
    "touch-interface",
    "react",
    "typescript"
  ],
  "author": {
    "name": "AeroFusionXR Team",
    "email": "team@aerofusionxr.com",
    "url": "https://aerofusionxr.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aerofusionxr/aerofusionxr.git",
    "directory": "clients/kiosk"
  },
  "bugs": {
    "url": "https://github.com/aerofusionxr/aerofusionxr/issues"
  }
