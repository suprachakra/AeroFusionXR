# AWS Credentials
AWS_ACCESS_KEY_ID=YOUR_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_SECRET_KEY
AWS_REGION=ap-southeast-1

# Database
DB_URI=postgresql://user:password@hostname:5432/aerofusion

# HSM / Key Management
HSM_ENDPOINT=https://hsm.example.com
HSM_KEY_ID=example-key-id

# Feature Flags
GDPR_FLAGS=true
ENABLE_SUSTAINABILITY=true
